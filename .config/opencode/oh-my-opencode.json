{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/refs/heads/dev/assets/oh-my-opencode.schema.json",
  "sisyphus_agent": {
    "disabled": false,
    "default_builder_enabled": false,
    "planner_enabled": true,
    "replace_plan": true
  },
  "disabled_mcps": [],
  "google_auth": true,
  "agents": {
    "atlas": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "prometheus": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "metis": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "momus": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "hephaestus": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "sisyphus": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "sisyphus-junior": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4"
    },
    "librarian": {
      "model": "kimi-for-coding/k2p5"
    },
    "explore": {
      "model": "kimi-for-coding/k2p5"
    },
    "oracle": {
      "model": "kimi-for-coding/k2p5"
    },
    "multimodal-looker": {
      "model": "kimi-for-coding/k2p5"
    },
    "build": {
      "model": "kimi-for-coding/k2p5"
    },
    "plan": {
      "model": "kimi-for-coding/k2p5"
    }
  },
  "categories": {
    "ultrabrain": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4",
      "variant": "max"
    },
    "artistry": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4",
      "variant": "max"
    },
    "unspecified-high": {
      "model": "synthetic/hf:nvidia/Kimi-K2.5-NVFP4",
      "variant": "max"
    },
    "unspecified-low": {
      "model": "opencode/minimax-m2.5-free"
    },
    "visual-engineering": {
      "model": "kimi-for-coding/k2p5"
    },
    "quick": {
      "model": "opencode/minimax-m2.5-free"
    },
    "writing": {
      "model": "opencode/minimax-m2.5-free"
    }
  },
  "tmux": {
    "enabled": true,
    "layout": "main-vertical"
  },
  "browser_automation_engine": {
    "provider": "agent-browser"
  },
  "lsp": {
    "typescript-language-server": {
      "command": ["typescript-language-server", "--stdio"],
      "extensions": [".ts", ".tsx"],
      "priority": 10
    },
    "pylsp": {
      "disabled": false
    }
  }
}
